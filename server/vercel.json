{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      }
    ],
    "redirects": [
      {
        "source": "/src(.*)",
        "destination": "/public/401.html"
      }
    ],
    "functions": {
        "api/index.js": {
        "memory": 1024,
        "maxDuration": 10
        }
    }
  }